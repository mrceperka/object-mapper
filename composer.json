{
	"name": "orisai/object-mapper",
	"description": "Raw data mapping to validated objects",
	"keywords": [
		"orisai",
		"object mapper",
		"validation",
		"data mapping"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/object-mapper",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"ext-mbstring": "*",
		"contributte/utils": "^0.4.2",
		"doctrine/annotations": "^1.10.3",
		"orisai/exceptions": "^1.0.0"
	},
	"require-dev": {
		"nette/caching": "^3.0",
		"nette/di": "^3.0.0",
		"orisai/coding-standard": "^1.0.0",
		"pepakriz/phpstan-exception-rules": "^0.10.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-deprecation-rules": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"phpstan/phpstan-strict-rules": "^0.12.0",
		"phpunit/phpunit": "^9.3.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.1.0"
	},
	"autoload": {
		"psr-4": {
			"Orisai\\ObjectMapper\\": "src"
		}
	},
	"autoload-dev": {
	  "psr-4": {
		"Tests\\Orisai\\ObjectMapper\\": "tests"
	  }
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
